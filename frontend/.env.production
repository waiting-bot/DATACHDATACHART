# 生产环境配置
VITE_API_URL=https://api.yourdomain.com
VITE_API_BASE_URL=/api/v1
VITE_API_TIMEOUT=30000

# 应用配置
VITE_APP_NAME=智能图表生成工具
VITE_APP_VERSION=1.0.0
VITE_DEBUG=false

# 文件上传配置
VITE_MAX_FILE_SIZE=10  # MB
VITE_SUPPORTED_FORMATS=.xlsx,.xls
VITE_MAX_UPLOAD_RETRIES=3

# 图表配置
VITE_DEFAULT_CHART_TYPE=bar
VITE_CHART_PREVIEW_WIDTH=400
VITE_CHART_PREVIEW_HEIGHT=300
VITE_CHART_FINAL_WIDTH=800
VITE_CHART_FINAL_HEIGHT=600

# UI配置
VITE_AUTO_ADVANCE_STEPS=true
VITE_SHOW_LOADING_SPINNER=true
VITE_ENABLE_ANIMATIONS=true

# 生产配置
VITE_DEV_MODE=false
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_ERROR_TRACKING=true